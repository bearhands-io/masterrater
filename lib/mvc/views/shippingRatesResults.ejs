<% if(ratingResults && ratingResults.length) { %>
	<br />
	Found <%= ratingResults.length %> results: <br /> <br />

	<% for(var i = 0; i < ratingResults.length; i++) { %>
		<% var result = ratingResults[i]; %>

		<%= result.carrier %> will ship from <%= result.fromZip %> for $<%= result.cost %> <br />

	<% } %>



<% } else { %>
	<br />
	No results returned!
<% } %>