  <div class="row">
    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading clearfix">
          <h3 class="panel-title">Found <%= ratingResults.length %> results:</h3>
        </div>
		<% if(ratingResults && ratingResults.length) { %>
			<br />
			<table class="table table-hover">
				<thead> 
		        <tr>
		          <th>Warehouse</th>
		          <th>Carrier</th>
		          <th>Cost</th>
		          <th>Status</th>
		        </tr>
		      	</thead>
		      	<tbody>
					<% for(var i = 0; i < ratingResults.length; i++) { %>
						<% var result = ratingResults[i]; %>

						<tr id='carrierrate' class='<% if (result.cost / orderprice < .03) { %>success<% } %>'>
							<td><%= result.fromZip%></td>
							<td><%= result.carrier %></td>
							<td>$<%= result.cost %></td>
							<td>ADD APPROVAL</td>
						</tr>

					<% } %>
				</tbody>
		    </table>


		<% } else { %>
			<br />
			No results returned!
		<% } %>
	  </div>
	</div>
  </div>